class TDevice:
    def __init__(self, tamaño, peso, altura, marca):
        self.tamaño = tamaño
        self.peso = peso
        self.altura = altura
        self.marca = marca
    
    def encender(self):
        print("El dispositivo se ha encendido.")

    def apagar(self):
        print("El dispositivo se ha apagado.")
